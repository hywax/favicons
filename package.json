{
  "name": "vite-vanilla-library",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "import": "./dist/vite-vanilla-library.mjs",
      "require": "./dist/vite-vanilla-library.cjs"
    }
  },
  "main": "./dist/vite-vanilla-library.cjs",
  "module": "./dist/vite-vanilla-library.js",
  "types": "./dist/vite-vanilla-library.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "release": "pnpm test && pnpm build && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint . --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@types/node": "^20.16.5",
    "@vitest/coverage-v8": "^1.6.0",
    "camelcase": "^8.0.0",
    "changelogen": "^0.5.5",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.ts": "pnpm run lint"
  }
}
